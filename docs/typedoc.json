{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "name": "API",
  "entryPoints": [
    "../packages/cli",
    "../packages/core",
    "../packages/fs",
    "../packages/oas",
    "../packages/parser-ts",
    "../packages/plugin-client",
    "../packages/plugin-faker",
    "../packages/plugin-msw",
    "../packages/plugin-oas",
    "../packages/plugin-react-query",
    "../packages/plugin-vue-query",
    "../packages/plugin-solid-query",
    "../packages/plugin-svelte-query",
    "../packages/plugin-redoc",
    "../packages/plugin-swr",
    "../packages/plugin-ts",
    "../packages/plugin-zod",
    "../packages/react",
    "../packages/types",
    "../packages/unplugin-kubb"
  ],
  "plugin": ["typedoc-plugin-markdown", "typedoc-vitepress-theme"],
  "entryPointStrategy": "packages",
  "exclude": "**/*+(index|.spec|.e2e).ts",
  "packageOptions": {
    "includeVersion": true,
    "entryPoints": ["./src/index.ts"]
  },
  "gitRevision": "main",
  "out": "./api",
  "indexFormat": "table",
  "useCodeBlocks": true,
  "textContentMappings": {
    "title.indexPage": "API Reference",
    "title.memberPage": "{name}"
  },
  "categorizeByGroup": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": true
  },
  "visibilityFilters": {
    "@interal": false,
    "@alpha": false,
    "@beta": false
  }
}
