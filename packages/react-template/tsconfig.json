{
  "extends": "@kubb/ts-config/base.json",
  "compilerOptions": {
    // "composite": true,
    // "rootDir": "./",
    // "outDir": "./dist",
    // "tsBuildInfoFile": "./build/.tsbuildinfo"
    "types": [
      "vitest/globals",
      "bun-types",
      "./globals",
      "../core/globals.d.ts",
      "../config/ts-config/reset.d.ts",
    ],
    "paths": {
      "@kubb/eslint-config": [
        "../config/eslint-config/src/index.ts"
      ],
      "@kubb/tsup-config": [
        "../config/tsup-config/src/index.ts"
      ],
      "@kubb/ts-config": [
        "../config/ts-config/src/index.ts"
      ],
      "@kubb/core": [
        "../core/src/index.ts"
      ],
      "@kubb/react-template/": [
        "./src/index.ts"
      ],
      "@kubb/react-template/jsx-runtime": [
        "src/jsx-runtime.ts"
      ],
      "@kubb/react-template/client": [
        "./src/client/index.ts"
      ],
    }
  },
  "include": [
    "src/**/*",
    "./package.json",
    "./tsup.config.ts",
    "./vitest.config.ts",
    "@kubb/ts-config/reset"
  ],
  "exclude": [
    "**/node_modules",
    "**/types/**",
    "**/mocks/**"
  ],
  "references": []
}