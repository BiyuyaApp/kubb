{
  "extends": "@kubb/ts-config/base.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "jsxImportSource": "@kubb/react",
    "types": [
      "vitest/globals",
      "bun-types",
      "@kubb/ts-config"
    ],
    "paths": {
      "@kubb/react/jsx-dev-runtime": [
        "./packages/react/src/jsx-runtime.ts"
      ]
    }
  },
  "include": [
    "./package.json",
    "./tsup.config.ts",
    "./vitest.config.ts",
    "./eslint.config.js"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/types/**"
  ]
}
