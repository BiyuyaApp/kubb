{
    "name": "advanced-pet-store",
    "version": "0.0.1",
    "description": "Advanced PetStore example",
    "repository": {
        "type": "git",
        "url": "git://github.com/stijnvanhulle/kubb.git",
        "directory": "packages/examples/advanced"
    },
    "license": "MIT",
    "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
    "sideEffects": false,
    "scripts": {
        "generate": "pnpm generate:ts",
        "generate:ts": "kubb --config kubb.config.ts",
        "generate:json": "kubb --config kubb.json",
        "generate:js": "kubb --config .kubbrc",
        "build": "tsup",
        "start": "tsup --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "typecheck": "tsc -p ./tsconfig.json --noEmit"
    },
    "dependencies": {
        "@kubb/cli": "workspace:*",
        "@kubb/core": "workspace:*",
        "@kubb/swagger": "workspace:*",
        "@kubb/swagger-react-query": "workspace:*",
        "@kubb/swagger-typescript": "workspace:*",
        "@tanstack/react-query": "^4.20.9",
        "axios": "^1.2.2",
        "react": "^18.2.0",
        "url-template": "^3.1.0"
    }
}