    "build": "tsup",
    "start": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@kubb/cli": "workspace:*",
    "@kubb/core": "workspace:*",
    "@kubb/swagger": "workspace:*",
    "@kubb/swagger-react-query": "workspace:*",
    "@kubb/swagger-ts": "workspace:*",
    "@kubb/swagger-client": "workspace:*",
    "@kubb/swagger-zod": "workspace:*",
    "@tanstack/react-query": "^4.28.0",
    "axios": "^1.3.4",
    "tsup": "^6.7.0",
    "react": "^18.2.0",
    "zod": "^3.21.4"
  }
}